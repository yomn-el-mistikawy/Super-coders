<h1>Listing group_members</h1>

<table>
  <thead>
    <tr>
      <th>User name</th>
      <th>Group name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @group_members.each do |group_member| %>
      <tr>
        <td><%= group_member.user_name %></td>
        <td><%= group_member.group_name %></td>
        <td><%= link_to 'Show Group', group_member %></td>
       
        <td><%= link_to 'leave group', group_member, method: :delete, data: { confirm: 'Are you sure you would like to leave group?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group member', new_group_member_path %>
